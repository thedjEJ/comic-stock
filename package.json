{
  "name": "comic-stock",
  "version": "0.1.0",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "private": true,
  "homepage": "https://thedjej.github.io/comic-stock",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "gh-pages": "^1.0.0",
    "prettier": "^1.5.3",
    "react": "^15.6.1",
    "react-alert": "^2.2.0",
    "react-bootstrap": "^0.31.2",
    "react-bootstrap-table": "^4.0.0",
    "react-confirm": "^0.1.14",
    "react-dom": "^15.6.1",
    "react-dropdown": "^1.2.5",
    "react-pacomo": "^0.5.3",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test:coverage": "echo 'Running coverage' && babel-node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha --require --recursive --colors src/test/*.test.js",
    "test": "mocha --nocache NODE_ENV=test --compilers js:babel-core/register src/test/*.test.js",
    "old-test": "react-scripts test --env=jsdom",
    "test-mocha": "./node_modules/mocha-webpack/bin/mocha-webpack --opts ./mocha-webpack.opts --webpack-config ./webpack.testing.config.js -r src/test/helpers/browser.js src/test/*.test.js",
    "compile": "babel --presets es2015,stage-0 -d lib/ src/"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "chai": "^4.1.1",
    "enzyme": "^2.9.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-prettier": "^2.2.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.0",
    "mocha-webpack": "^0.7.0",
    "sinon": "^3.2.1"
  }
}
