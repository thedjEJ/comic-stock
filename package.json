{
  "name": "comic-stock",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://thedjej.github.io/comic-stock",
  "dependencies": {
    "axios": "^0.16.2",
    "babel": {
      "presets": [
        "es2015",
        "react"
      ]
    },
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2016": "^6.24.1",
    "gh-pages": "^1.0.0",
    "prettier": "^1.5.3",
    "proptypes": "^1.1.0",
    "react": "^15.6.1",
    "react-alert": "^2.2.0",
    "react-bootstrap": "^0.31.2",
    "react-bootstrap-table": "^4.0.0",
    "react-confirm": "^0.1.14",
    "react-dom": "^15.6.1",
    "react-dropdown": "^1.2.5",
    "react-modal": "^2.2.4",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test-nyc": "nyc mocha --compilers js:babel-register --reporter tap ./src/test/*.test.jsx",
    "test": "mocha --require src/test/helpers/testHelper.js -w src/test/helpers/browser.js --require babel-register --compilers js:babel-core/register src/test/*.test.jsx",
    "dev:hot": "webpack-dev-server --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./",
    "old-test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-root-slash-import": "^1.1.0",
    "chai": "^4.1.1",
    "enzyme": "^2.9.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-prettier": "^2.2.0",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "react-addons-test-utils": "^15.6.0"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ],
    "cache": true,
    "require": [
      "./src/test/helpers/browser.js"
    ]
  }
}
